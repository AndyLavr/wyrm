{
  "name": "wyrm",
  "version": "0.0.1",
  "homepage": "https://github.com/snide/wyrm",
  "authors": [
    "Dave Snider <dave.snider@gmail.com>"
  ],
  "description": "The wyrm frontend is a collection of responsive, static assets that work well with form or document heavy sites.",
  "license": "MIT",
  "private": true,
  "main": [
    "sass/*.sass",
  ], 
  "ignore": [
    "docs",
    ".gitignore",
    ".DS_Store",
